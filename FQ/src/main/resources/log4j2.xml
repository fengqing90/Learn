<?xml version="1.0" encoding="UTF-8"?>
<Configuration monitorInterval="180">

	<properties>
		<property name="LOG_HOME">C:\Users\fengqing.UCREDIT\Desktop\LOGS\logs</property>
	</properties>

	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<!--<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>-->
			<!--<PatternLayout pattern="%d{HH:mm:ss.SSS} %5p %-40.40logger{39} 【%-50m】 \(%F:%L\) [%t] %n"/>-->
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %p \(%F:%L\) %m [%t]  %n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info" includeLocation="true">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>